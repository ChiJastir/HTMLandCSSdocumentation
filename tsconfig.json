{
    "compilerOptions": {
        "jsx": "react-jsx",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "@entities/*": [
                "src/entities/*"
            ],
            "@features/*": [
                "src/features/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@shared/*": [
                "src/shared/*"
            ],
            "@widgets/*": [
                "src/widgets/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@app/*": [
                "src/app/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@newPages/*": [
                "src/newPages/*"
            ],
            "@router/*": [
                "src/app/router/*"
            ],
            "@styles/*": [
                "src/shared/styles/*"
            ],
            "@UI/*": [
                "src/UI/*"
            ],
            "@cssPages/*": [
                "src/pages/cssPages/*"
            ],
            "@htmlPages/*": [
                "src/pages/htmlPages/*"
            ],
            "@mainPage/*": [
                "src/pages/mainPage/*"
            ],
            "@page404/*": [
                "src/pages/page404/*"
            ],
            "@homeButton/*": [
                "src/shared/homeButton/*"
            ]
        }
    }
}